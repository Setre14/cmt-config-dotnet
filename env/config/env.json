{
  "apiVersion": "v1",
  "kind": "env-config",
  "spec": {
    "paths": [
      {
        "volume": "/",
        "profile": "common",
        "path": "user-home",
        "file": ".bashrc"
      },
      {
        "volume": "/",
        "profile": "common",
        "path": "user-home/.config",
        "file": "nvim"
      },
      {
        "volume": "/",
        "profile": "common",
        "path": "user-home/.config",
        "file": "env"
      },
      {
        "volume": "/",
        "profile": "common",
        "path": "user-home/.config",
        "file": "starship.toml"
      }
    ],
    "valuesFile": {
      "path": "env/values",
      "name": "values",
      "ext": "json"
    }
  }
}